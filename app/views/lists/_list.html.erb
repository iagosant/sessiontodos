<div id="main_center" data-list-id = "<%= list.id%>">
    <div class="center row">
      <div class="col s12">
        <% unless (list.all_tasks?) %>
            <div class="share-icons">
                <div class="row list-collaboration-users">
                    <div class="right">
                        <div id="collaboration-users">
                          <% if has_collaborations?(list) %>
                              <% if (!current_user.owner?(list)) %>
                                 <%= render partial: "collaboration_user", locals: {collaboration_user: list.owner} %>
                              <% end %>
                              <%= render partial: "collaboration_user", collection: list.collaboration_users.where(['user_id != ?', current_user.id]) %>
                          <% end %>
                        </div>
                      <% if (current_user.owner?(list)) %>
                            <div class="chip circle ms-text-green ms-add">
                              <%= link_to new_list_invitation_path(list),id: 'new_initation_link', remote: true, data: {behavior: 'modal'} do%>
                                <i class="material-icons ms-text-green add-user">add</i>
                              <% end %>
                            </div>
                      <% end %>
                    </div>
                </div>
            </div>
        <% end %>
      </div>
    </div>
    <div class="row">
      <div class="col s12" id="list-users">
          <%= render partial: "users/list_user", locals: {user: current_user} %>
      </div>
      <!-- <div id="show_collaboration_users" class="col s12">
      </div> -->
    </div>
</div>
