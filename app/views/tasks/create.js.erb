
 <% if params[:type].present? %>

   $('#blockers_task<%= @task.id%>').append('<%= j render partial: "t_blocker", object: @blocker %>');
   $('#edit_task_<%= @blocker.id %>').submitOnCheck();
   $('.show_blockers #detail').val('');
<% else %>

   $('#incomplete_tasks').append('<%= j render(@task) %>');
   $('#edit_task_<%= @task.id %>').submitOnCheck();
   $('.new_task #detail').val('');
 <% end %>
