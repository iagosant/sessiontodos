 <% if params[:type].present? %>
  var $user = $('#collaboration_users div#<%= @user.id %>');
  console.log($user);
  if ($user.hasClass('active')) {
      $user.removeClass('active');
      $('#show_collaboration_users #list_user_<%= @user.id %>').remove();
      console.log('remove active');
  } else {
      console.log($('#application_date .datepicker').val());
      $user.addClass('active');
      $('#show_collaboration_users').append("<%= j render partial: 'users/list_user', locals:{user: @user, list: @list } %>");
      $('#show_collaboration_users #list_user_<%= @user.id %> input[id="task_user_id"]').val("<%= @user.id %>");
      $('.edit_task').submitOnCheck();
   }
 <% end %>
